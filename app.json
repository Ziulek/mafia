{
  "expo": {
    "name": "Mafia",
    "slug": "mafia",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mafia",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mafia"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/AmericanTypewriter.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8aefe1ac-8ec5-461a-89dc-9a05daf812e6"
      }
    },
    "owner": "smakolysze",
    "android": {
      "package": "com.mafia",
      "softwareKeyboardLayoutMode": "pan"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8aefe1ac-8ec5-461a-89dc-9a05daf812e6"
    }
  }
}
